{
  "type": "json_schema",
  "json_schema": {
    "name": "module_2b_ClassifyOwnBrandKeywords_output",
    "strict": true,
    "schema": {
      "type": "object",
      "properties": {
        "branding_scope_1": {
          "type": ["string", "null"],
          "enum": ["OB", null],
          "description": "OB if keyword matches own brand, null otherwise"
        },
        "matched_term": {
          "type": ["string", "null"],
          "description": "The brand term found in the keyword"
        },
        "match_type": {
          "type": ["string", "null"],
          "enum": ["brand_variation", "sub_brand", "manufacturer", "standard", null],
          "description": "Type of match found"
        },
        "confidence": {
          "type": "number",
          "description": "Confidence score from 0.0 to 1.0"
        },
        "reasoning": {
          "type": "string",
          "description": "Brief explanation of why this classification was chosen"
        }
      },
      "required": ["branding_scope_1", "matched_term", "match_type", "confidence", "reasoning"],
      "additionalProperties": false
    }
  }
}
